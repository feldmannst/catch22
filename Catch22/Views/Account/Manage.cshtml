@model Catch22.Models.UpdateProfileViewModel
@using Catch22.Models;
@using Microsoft.AspNet.Identity;
@{
    ViewBag.Title = "Manage Account";
    Layout = "~/Views/Shared/_CMSLayout.cshtml";
}

<h2>@ViewBag.Title.</h2>

<p class="text-success">@ViewBag.StatusMessage</p>
<div class="row">
    <div class="col-md-12">
        @Html.Partial("_ChangePasswordPartial", Model.ManageViewModel)
        <hr />
        @Html.Partial("_UpdateProfile", Model)
    </div>
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
